Host *
	ControlPath ~/.ssh/.controlpath_%r@%h:%p
	ControlMaster auto
	ControlPersist 30m
Host github
	Hostname github.com
	User git
Host gitlab
	Hostname gitlab.com
	User git
Host aur
	Hostname aur.archlinux.com
	User aur
	IdentityFile ~/.ssh/aur
Host pi
	Hostname gereon.duckdns.org
	Port 36422
	User alarm
Host router
	Hostname gereon.duckdns.org
	Port 36422
	User alarm
	LocalForward 36480 192.168.0.1:80
	SessionType none
